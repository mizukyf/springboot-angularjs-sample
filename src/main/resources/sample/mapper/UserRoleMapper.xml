<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample.mapper.UserRoleMapper">
	<insert id="insert">
		INSERT INTO 
			tab_user_role 
			(user_id, role_id) 
		VALUES 
			(#{uid}, #{rid}) 
	</insert>
	<delete id="deleteByUserId">
		DELETE 
		FROM 
			tab_user_role 
		WHERE 
			user_id = #{uid} 
	</delete>
</mapper>